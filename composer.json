{
    "name": "omar/wp-visualization-plugins",
    "description": "A plugin for loading in several visualization type Gutenburg blocks.",
    "type": "project",
    "require-dev": {
        "phpunit/phpunit": "^9",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "wp-coding-standards/wpcs": "^2.3"
      },
    "license": "MIT",
    "autoload": {
		"classmap": [
			"src/"
		]
	},
    "autoload-dev": {
		"psr-4": {
			"WP_VIS_PLUGIN\\Tests\\": "tests/"
		}
	},
    "scripts": {
        "phpcs": "vendor/bin/phpcs",
        "phpcbf": "vendor/bin/phpcbf",
        "lint": "phpcs",
        "phpunit": "vendor/bin/phpunit",
        "test": "phpunit"
    },
    "authors": [
        {
            "name": "Omar Serrano",
            "email": "omar.serrano@automattic.com"
        }
    ],
    "require": {},
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
